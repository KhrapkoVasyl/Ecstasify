{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "echo \"Running tests\"",
    "eslint:check": "eslint --ignore-path .gitignore \"src/**/*.{ts,tsx,js,jsx}\"",
    "eslint:fix": "eslint --ignore-path .gitignore \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\" --ignore-path .gitignore",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\" --ignore-path .gitignore",
    "lint:check": "npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "lint:precommit": "npm run lint:fix",
    "prepare": "cd .. && husky install client/.husky"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.15.3",
    "axios": "^1.1.3",
    "howler": "^2.2.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "lodash.debounce": "^4.0.8",
    "mobx": "^6.6.2",
    "mobx-persist-store": "^1.1.3",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.1",
    "react-router-dom": "^6.4.3",
    "react-toastify": "^9.1.1",
    "typeface-jost": "^1.1.13",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/howler": "^2.2.7",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "vite": "^4.3.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx,json}": [
      "npm run lint:precommit"
    ]
  }
}
